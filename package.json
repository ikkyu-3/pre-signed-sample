{
  "name": "pre-signed-sample",
  "version": "0.0.0",
  "description": "ブラウザからS3にPre-Signed URLを使用してファイルをアップロードやダウンロードができるサンプル",
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack-dev-server",
    "build:preSignedUrl": "babel src/preSignedUrl -d src/preSignedUrl --extensions '.js,.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikkyu-3/pre-signed-sample.git"
  },
  "author": "Toru Hasegawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ikkyu-3/pre-signed-sample/issues"
  },
  "homepage": "https://github.com/ikkyu-3/pre-signed-sample#readme",
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-decorators": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/dotenv": "6.1.1",
    "@types/react": "16.8.14",
    "@types/react-css-modules": "4.6.2",
    "@types/react-dom": "16.8.4",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "babel-loader": "8.0.5",
    "css-loader": "2.1.1",
    "dotenv": "7.0.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0",
    "style-loader": "0.23.1",
    "typescript": "3.4.4",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.3.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "aws-sdk": "^2.441.0",
    "core-js": "^3.0.1",
    "react": "^16.8.6",
    "react-css-modules": "^4.7.9",
    "react-dom": "^16.8.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
